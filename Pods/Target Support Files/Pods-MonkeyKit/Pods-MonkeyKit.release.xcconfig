CLANG_CXX_LANGUAGE_STANDARD = compiler-default
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/AFNetworking.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Realm.framework/Headers"
OTHER_CPLUSPLUSFLAGS = -std=c++1y $(inherited)
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AFNetworking" -framework "Realm"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-MonkeyKit
PODS_ROOT = ${SRCROOT}/Pods